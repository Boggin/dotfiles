[core]
    autocrlf = false
    safecrlf = false
    editor = 'C:/program files (x86)/vim/vim74/gvim.exe' -f
    whitespace = cr-at-eol
    excludesfile = ~/.gitignore
[alias]
    ci = commit
    cm = commit -m
    cam = commit -am
    co = checkout
    df = diff
    dfc = diff --cached
    dft = diff --text
    st = status -sb
    br = branch
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    hist = log --graph --full-history --pretty="%x1b[31m%h%x20%x1b[32m%ar%x1b[0m%x20%s%x20%C(bold)%d%Creset%x20%x1b[33m(%an)%x1b[0m"
    history = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    ls = ls-files
    mn = merge --no-commit
    spull = !git-svn rebase
    spush = !git-svn dcommit
    tpull = "!git tfs fetch; git rebase tfs/default"
    tpush = "!git tfs ct"
[color]
    ui = true
[diff]
    tool = kdiff3
    mnemonicprefix = true
[difftool "kdiff3"]
    path = "C:/program files/kdiff3/kdiff3.exe"
    cmd = "kdiff3.exe $LOCAL $REMOTE"
    prompt = false
[merge]
    tool = kdiff3
    keepBackup = false
    stat = true
[mergetool "kdiff3"]
    path = "C:/program files/kdiff3/kdiff3.exe"
    cmd = "kdiff3.exe $BASE $LOCAL $REMOTE $MERGED"
    trustExitCode = true
[http]
    #proxy = http://127.0.0.1:3128
    #postBuffer = 524288000
    #receivepack = true
[https]
    #proxy = https://127.0.0.1:3128
    #postBuffer = 524288000
    #receivepack = true
[advice]
    statusHints = false
[branch]
    autosetupmerge = true
    autosetuprebase = always
[push]
    default = tracking
[help]
    format = web
[web]
    browser = chrome
[browser "chrome"]
    path = "C:\\program files (x86)\\Google\\Chrome\\Application\\chrome.exe"
[browser "firefox"]
    path = "C:\\program files (x86)\\Mozilla Firefox\\firefox.exe"
[rerere]
	enabled = true
	autoupdate = true
